{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "concat_space": {
            "spacing": "one"
        },
        "php_unit_method_casing": {
            "case": "camel_case"
        }
    },
    "exclude": [
        "vendor",
        "storage",
        "bootstrap/cache",
        "node_modules"
    ]
}
