{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "app"
        ],
        "excludes": [
            "Exceptions",
            "Providers"
        ]
    },
    "logs": {
        "text": "storage/infection/infection.log",
        "html": "storage/infection/infection.html",
        "summary": "storage/infection/summary.log",
        "json": "storage/infection/infection.json",
        "perMutator": "storage/infection/per-mutator.md",
        "github": true
    },
    "tmpDir": "storage/infection",
    "timeout": 10,
    "phpUnit": {
        "configDir": ".",
        "customPath": "vendor/bin/phpunit"
    },
    "testFramework": "phpunit",
    "bootstrap": "vendor/autoload.php",
    "mutators": {
        "@default": true,
        "@function_signature": false,
        "MethodCallRemoval": {
            "ignore": [
                "Illuminate\\Support\\Facades\\Log::*"
            ]
        }
    },
    "minMsi": 80,
    "minCoveredMsi": 90,
    "threads": 4,
    "ignoreMsiWithNoMutations": true,
    "ignoreSourceCodeByRegex": [
        ".*Test\\.php$"
    ]
}
