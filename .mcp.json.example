{
	"mcpServers": {
		"chrome-devtools": {
			"command": "npx",
			"args": ["chrome-devtools-mcp@latest"]
		},
		"laravel-mcp-companion": {
			"command": "docker",
			"args": ["run", "--rm", "-i", "ghcr.io/brianirish/laravel-mcp-companion:latest"]
		},
		"task-master-ai": {
			"command": "npx",
			"args": ["-y", "task-master-ai"],
			"env": {
				"OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}"
			}
		}
	}
}
